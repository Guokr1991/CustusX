# Doxyfile 1.3.9.1

PROJECT_NAME           = CustusX3
PROJECT_NUMBER         = @CustusX3_VERSION_MAJOR@.@CustusX3_VERSION_MINOR@.@CustusX3_VERSION_PATCH@
OUTPUT_DIRECTORY       = @CustusX3_BINARY_DIR@/doc/doxygen
SHORT_NAMES            = @DOXYGEN_USE_SHORT_NAMES@

INPUT                  = "@CustusX3_SOURCE_DIR@/Source" 

JAVADOC_AUTOBRIEF      = YES
MULTILINE_CPP_IS_BRIEF = YES

EXTRACT_ALL            = YES
EXTRACT_PRIVATE        = YES
EXTRACT_STATIC         = YES
EXTRACT_LOCAL_CLASSES  = NO

SORT_MEMBER_DOCS       = YES
SORT_BRIEF_DOCS        = NO
GENERATE_TODOLIST      = YES
GENERATE_BUGLIST       = YES
SHOW_DIRECTORIES       = YES

QUIET		       = YES
WARNINGS               = @DOXYGEN_USE_WARNINGS@
WARN_FORMAT            = "Warning: Doxygen -- $file:$line:  $text"
WARN_IF_UNDOCUMENTED   = NO
WARN_IF_DOC_ERROR      = YES

RECURSIVE              = YES
EXCLUDE                = "@CustusX3_SOURCE_DIR@/Source/igstk"

SOURCE_BROWSER         = YES
INLINE_SOURCES         = NO
REFERENCED_BY_RELATION = YES
REFERENCES_RELATION    = YES
VERBATIM_HEADERS       = YES
ALPHABETICAL_INDEX     = YES
COLS_IN_ALPHA_INDEX    = 3

GENERATE_LATEX         = NO
GENERATE_HTML          = YES
HTML_OUTPUT            = .
HTML_FILE_EXTENSION    = .html
DISABLE_INDEX          = NO

ENABLE_PREPROCESSING   = YES
MACRO_EXPANSION        = YES
EXPAND_ONLY_PREDEF     = NO

CLASS_DIAGRAMS         = YES
HAVE_DOT               = @HAVE_DOT_YESNO@
CLASS_GRAPH            = YES
COLLABORATION_GRAPH    = YES
TEMPLATE_RELATIONS     = YES
INCLUDE_GRAPH          = YES
INCLUDED_BY_GRAPH      = YES
GRAPHICAL_HIERARCHY    = YES
DOT_IMAGE_FORMAT       = png
DOT_PATH               = "@DOT_PATH@"

GENERATE_LEGEND        = YES
DOT_CLEANUP            = YES
